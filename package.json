{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "porfolio-jacques-beets",
  "main": "index.js",
  "dependencies": {
    "autoprefixer": "^9.1.5",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0"
  },
  "devDependencies": {},
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "prefix:css": "postcss css/style.css --use autoprefixer -o css/style.prefix.css",
    "start": "npm-run-all --parallel devserver watch:sass",
    "build:css": "npm-run-all prefix:css"
  },
